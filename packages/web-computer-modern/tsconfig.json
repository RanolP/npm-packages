{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2024",
    "lib": ["ES2023.Array", "ESNext.Array"],
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "strict": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["scripts/*.js"],
  "exclude": []
}
